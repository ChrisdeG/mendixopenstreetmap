<?xml version="1.0" encoding="utf-8"?>
<widget id="lowcodeconnect.openstreetmap.Openstreetmap" pluginWidget="true" needsEntityContext="true" offlineCapable="true" supportedPlatform="Web"
    xmlns="http://www.mendix.com/widget/1.0/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd">
    <name>Openstreetmap</name>
    <description>Leaflet</description>
    <icon></icon>
    <properties>
        <propertyGroup caption="General">
            <property key="zoom" type="expression" required="true" defaultValue='13'>
                <caption>Zoom</caption>
                <description>Zoom value 0=world 18=detailed</description>
                <returnType type="Integer"/>
            </property>
            <property key="centerlatitude" type="expression" required="false">
                <caption>Center latitude</caption>
                <description>Center latitude</description>
                <returnType type="Decimal"/>
            </property>
            <property key="centerlongitude" type="expression" required="false">
                <caption>Center longitude</caption>
                <description>Center longitude</description>
                <returnType type="Decimal"/>
            </property>
            <property key="height" type="expression" required="true" defaultValue='80vh'>
                <caption>Height</caption>
                <description>Use fixed px or 80vh for 80% of viewheight</description>
                <returnType type="String"/>
            </property>
            <property key="width" type="expression" required="true" defaultValue='100%'>
                <caption>Width</caption>
                <description>Use fixed px or 80vh for 80% of viewheight</description>
                <returnType type="String"/>
            </property>
            <property key="scrollwheelzoom" type="boolean" required="true" defaultValue="false">
                <caption>Scroll wheel zoom</caption>
                <description>Use the scroll wheel to scroll</description>
            </property>
        </propertyGroup>
        <propertyGroup caption="Routes">
            <property key="routesentity" type="datasource" isList="true" required="false">
                <caption>Routes entity</caption>
                <description>Datasource containing routes</description>
            </property>
            <property key="routecolor" type="expression" dataSource="routesentity">
                <caption>Color</caption>
                <description>Color of the route</description>
                <returnType type="String" />
            </property>
        </propertyGroup>
        <propertyGroup caption="Waypoints">
            <property key="pointsentity" type="datasource" isList="true" required="false">
                <caption>Waypoints entity</caption>
                <description>Waypoints of a route, which should have latitude and longitude attributes.</description>
            </property>
            <property key="pointstoroute" type="association" dataSource="pointsentity" selectableObjects="routesentity">
                <caption>Association to routes</caption>
                <description>Assocation to route</description>
                <associationTypes>
                    <associationType name="Reference" />
                    <associationType name="ReferenceSet"/>
                </associationTypes>
            </property>
            <property key="lonattribute" type="attribute" dataSource="pointsentity" required="true">
                <caption>Longitude attribute</caption>
                <description>Attribute containing the longitude</description>
                <attributeTypes>
                    <attributeType name="Decimal" />
                </attributeTypes>
            </property>
            <property key="latattribute" type="attribute" dataSource="pointsentity" required="true">
                <caption>Latitude attribute</caption>
                <description>Attribute containing the latitude</description>
                <attributeTypes>
                    <attributeType name="Decimal" />
                </attributeTypes>
            </property>
            <property key="pointsortorderattr" type="attribute" dataSource="pointsentity" required="true">
                <caption>Order attribute</caption>
                <description>Sorting the waypoints</description>
                <attributeTypes>
                    <attributeType name="Decimal" />
                    <attributeType name="Integer" />
                </attributeTypes>
            </property>
        </propertyGroup>
        <propertyGroup caption="Events">
            <property key="onClickAction" type="action" required="false">
                <caption>On click</caption>
                <description>When the map is clicked</description>
            </property>
        </propertyGroup>
        <propertyGroup caption="Markers">
            <property key="markersentity" type="datasource" isList="true" required="false">
                <caption>Markers entity</caption>
                <description>Markers</description>
            </property>
            <property key="markerlonattr" type="attribute" dataSource="markersentity" required="false">
                <caption>Longitude attribute</caption>
                <description>Attribute containing the longitude</description>
                <attributeTypes>
                    <attributeType name="Decimal" />
                </attributeTypes>
            </property>
            <property key="markerlatattr" type="attribute" dataSource="markersentity" required="true">
                <caption>Latitude attribute</caption>
                <description>Attribute containing the latitude</description>
                <attributeTypes>
                    <attributeType name="Decimal" />
                </attributeTypes>
            </property>
            <property key="markercolorattr" type="attribute" dataSource="markersentity" required="false">
                <caption>Color attribute</caption>
                <description>Default blue, Allowed values are blue, gold, red, green, orange, yellow, violet, grey, black</description>
                <attributeTypes>
                    <attributeType name="String" />
                </attributeTypes>
            </property>
            <property key="markerlabelattr" type="attribute" dataSource="markersentity" required="false">
                <caption>Label attribute</caption>
                <description>Attribute containing the label of the marker</description>
                <attributeTypes>
                    <attributeType name="String" />
                </attributeTypes>
            </property>
            <property key="markerAction" type="action" dataSource="markersentity" required="false">
                <caption>On click marker</caption>
                <description>Action when the marker is clicked</description>
            </property>
        </propertyGroup>
        <propertyGroup caption="Add marker">
            <property key="lonclickattr" type="attribute" required="false">
                <caption>Longitude attribute</caption>
                <description>Attribute containing the longitude for added Marker</description>
                <attributeTypes>
                    <attributeType name="Decimal" />
                </attributeTypes>
            </property>
            <property key="latclickattr" type="attribute" required="false">
                <caption>Latitude attribute</caption>
                <description>Attribute containing the latitude for added Marker</description>
                <attributeTypes>
                    <attributeType name="Decimal" />
                </attributeTypes>
            </property>
        </propertyGroup>
    </properties>
</widget>
